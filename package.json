{
    "name": "osu-stuff",
    "version": "2.2.0",
    "description": "yet another osu! manager",
    "homepage": "https://github.com/mezleca/osu-stuff",
    "main": "./out/main/index.js",
    "license": "MIT",
    "author": "rel",
    "scripts": {
        "format": "prettier --plugin prettier-plugin-svelte --write .",
        "lint": "eslint --cache .",
        "svelte-check": "svelte-check --tsconfig ./tsconfig.json",
        "start": "bun rebuild && electron-vite preview",
        "dev": "bun rebuild && cross-env NODE_ENV=dev electron-vite dev",
        "build": "bun rebuild && electron-vite build",
        "rebuild": "electron-rebuild -f -w better-sqlite3",
        "postinstall": "electron-builder install-app-deps",
        "test": "npm rebuild && cross-env NODE_ENV=test vitest ./tests/main/",
        "test:drivers": "npm rebuild && cross-env NODE_ENV=test vitest ./tests/main/drivers.test.ts",
        "test:processor": "npm rebuild && cross-env NODE_ENV=test vitest ./tests/main/processor.test.ts",
        "build:unpack": "bun run build && electron-builder --dir",
        "build:win": "bun run build && electron-builder --win",
        "build:mac": "bun run build && electron-builder --mac",
        "build:linux": "bun run build && electron-builder --linux"
    },
    "dependencies": {
        "@electron-toolkit/preload": "^3.0.2",
        "@electron-toolkit/utils": "^4.0.0",
        "@rollup/plugin-node-resolve": "^16.0.3",
        "better-sqlite3": "^12.4.6",
        "jszip": "^3.10.1",
        "osu-api-extended": "^3.1.7",
        "realm": "^20.2.0",
        "vite-tsconfig-paths": "^5.1.4",
        "vitest": "^4.0.13"
    },
    "devDependencies": {
        "@electron-toolkit/eslint-config-prettier": "^3.0.0",
        "@electron-toolkit/eslint-config-ts": "^3.1.0",
        "@electron-toolkit/tsconfig": "^2.0.0",
        "@sveltejs/enhanced-img": "^0.8.5",
        "@sveltejs/vite-plugin-svelte": "^6.2.0",
        "@types/better-sqlite3": "^7.6.13",
        "@types/jszip": "^3.1.6",
        "@types/node": "^22.18.6",
        "cross-env": "^10.1.0",
        "electron": "^39.2.3",
        "electron-builder": "^25.1.8",
        "electron-vite": "^4.0.1",
        "eslint": "^9.36.0",
        "eslint-plugin-svelte": "^3.12.4",
        "node-addon-api": "^8.5.0",
        "node-api-headers": "^1.6.0",
        "prettier": "^3.6.2",
        "prettier-plugin-svelte": "^3.4.0",
        "svelte": "^5.43.14",
        "svelte-check": "^4.3.4",
        "typescript": "^5.9.3",
        "vite": "^7.2.4"
    }
}
