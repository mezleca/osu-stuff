{
  "name": "osu-stuff",
  "productName": "osu-stuff",
  "version": "v1.0.1",
  "description": "osu stuff",
  "main": "src/index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=cleide electron .",
