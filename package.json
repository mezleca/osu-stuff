{
    "name": "osu-stuff",
    "version": "2.4.2",
    "description": "yet another osu! manager",
    "homepage": "https://github.com/mezleca/osu-stuff",
    "main": "./out/main/index.js",
    "license": "MIT",
    "author": "rel",
    "scripts": {
        "format": "prettier --plugin prettier-plugin-svelte --write .",
        "lint": "bunx tsc --noEmit",
        "svelte-check": "svelte-check --tsconfig ./tsconfig.json",
        "start": "bun rebuild && bun scripts/cli.ts start",
        "dev": "bun rebuild && bun scripts/cli.ts dev",
        "build": "bun rebuild && bun scripts/cli.ts build",
        "rebuild": "electron-rebuild -f",
        "postinstall": "electron-builder install-app-deps",
        "test": "bun rebuild && cross-env NODE_ENV=test vitest ./tests/main/",
        "test:drivers": "bun rebuild && cross-env NODE_ENV=test vitest ./tests/main/drivers.test.ts",
        "test:processor": "bun rebuild && cross-env NODE_ENV=test vitest ./tests/main/processor.test.ts",
        "build:unpack": "bun run build && electron-builder --dir",
        "build:win": "bun run build && electron-builder --win",
        "build:linux": "bun run build && electron-builder --linux"
    },
    "dependencies": {
        "@electron-toolkit/utils": "^4.0.0",
        "@rel-packages/osu-beatmap-parser": "^0.1.9",
        "@rel-packages/osu-beatmap-preview": "^0.4.0",
        "@types/archiver": "^7.0.0",
        "archiver": "^7.0.1",
        "better-sqlite3": "^12.6.2",
        "electron-updater": "^6.7.3",
        "ms": "^2.1.3",
        "osu-api-extended": "^3.1.7",
        "quick-lru": "^7.3.0",
        "realm": "^20.2.0"
    },
    "devDependencies": {
        "@electron-toolkit/eslint-config-prettier": "^3.0.0",
        "@electron-toolkit/eslint-config-ts": "^3.1.0",
        "@electron-toolkit/preload": "^3.0.2",
        "@electron-toolkit/tsconfig": "^2.0.0",
        "@sveltejs/vite-plugin-svelte": "^6.2.1",
        "@types/better-sqlite3": "^7.6.13",
        "@types/bun": "^1.3.4",
        "@types/jszip": "^3.4.1",
        "@types/node": "^25.0.1",
        "cross-env": "^10.1.0",
        "electron": "^40.1.0",
        "electron-builder": "^26.7.0",
        "prettier": "^3.7.4",
        "prettier-plugin-svelte": "^3.4.0",
        "svelte": "^5.49.1",
        "svelte-check": "^4.3.4",
        "svelte-preprocess": "6.0.3",
        "typescript": "^5.9.3",
        "vite": "^7.2.7",
        "vite-tsconfig-paths": "^6.0.1",
        "vitest": "^4.0.15"
    }
}
