{
  "name": "osu-stuff",
  "productName": "osu-stuff",
  "version": "v1.3.0",
  "description": "just another app to manage osu! stable collections and download beatmaps ",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "npm run build && electron .",
    "build": "webpack",
    "dev": "cross-env NODE_ENV=development electron .",
    "make": "npm run build && electron-builder",
    "make:win": "npm run build && electron-builder --win",
    "make:linux": "npm run build && electron-builder --linux"
  },
  "devDependencies": {
    "@electron/fuses": "^1.8.0",
    "@electron/packager": "^18.3.6",
    "autoprefixer": "^10.4.20",
    "cross-env": "^7.0.3",
    "electron": "34.2.0",
    "electron-builder": "^25.1.8",
    "electron-installer-squirrel-windows": "^1.3.0",
    "webpack": "^5.98.0",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "electron-localshortcut": "^3.2.1",
    "electron-squirrel-startup": "^1.0.1"
  },
  "build": {
    "appId": "com.osustuff.app",
    "productName": "osu-stuff",
    "asar": true,
    "compression": "maximum",
    "directories": {
      "buildResources": "build",
      "output": "dist"
    },
    "files": [
      "src/**/*",
      "package.json"
    ],
    "win": {
      "target": "nsis",
      "icon": "build/icons/win/icon.ico",
      "requestedExecutionLevel": "requireAdministrator"
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ],
      "icon": "build/icons/png",
      "category": "Utility"
    },
    "nsis": {
      "artifactName": "osu-stuff-${version}.${ext}",
      "guid": "c20afe8f-91d5-4b12-aed6-88684e4c7ed3",
      "oneClick": true,
      "perMachine": true
    }
  },
  "author": {
    "name": "rel",
    "email": "92015390+mezleca@users.noreply.github.com"
  },
  "license": "MIT"
}
